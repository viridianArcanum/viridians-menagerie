{
	"name": "Flight",
	"description": "You have wings and can use them to fly. Sprint while in elytra flight to gain forward propulsion.",
	"type": "origins:multiple",
	"elytra": {
		"type": "origins:elytra_flight",
		"render_elytra": true,
		"texture_location": "minecraft:textures/entity/elytra.png"
	},
	"resource": {
		"type": "origins:resource",
		"min": 0,
		"max": 30,
		"start_value": 30,
		"hud_render": {
			"should_render": true,
			"bar_index": 4,
			"condition": {
                "type": "origins:resource",
                "resource": "*:*_resource",
                "comparison": "<",
                "compare_to": 30
            }
		}
	},
	"gain": {
		"type": "origins:action_over_time",
		"interval": 20,
		"entity_action": {
			"type": "origins:change_resource",
			"resource": "*:*_resource",
			"change": 1
		}
	},
	"use": {
		"type": "origins:active_self",
		"key": {
			"key": "key.sprint",
			"continuous": true
		},
		"entity_action": [
			{
				"type": "origins:add_velocity",
				"z": 0.0825,
				"space": "local"
			},
			{
				"type": "origins:change_resource",
				"resource": "*:*_resource",
				"change": -1
			},
			{
				"type": "origins:exhaust",
				"interval": 4,
				"amount": 0.6
			}
		],
		"condition": {
			"type": "origins:all_of",
			"conditions": [
				{
					"type": "origins:fall_flying"
				},
				{
					"type": "origins:resource",
					"resource": "*:*_resource",
					"comparison": ">",
					"compare_to": 0
				}
			]
		}
	}
}